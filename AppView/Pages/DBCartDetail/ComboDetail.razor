@page "/cartdetail/products/{id}"
@using AppData.Models
@using AppData.ViewModes.Combo
@inject IComboDetailServices _combodetailService
@inject IProductDetailService _productDetailService
@inject NavigationManager _navigtion;

<div class="card">
    <div class="card-header text-center">
        <h3>Chi tiết</h3>
    </div>
    <div class="card-body">
        <div class="row">
            @if (lstDetailCombo != null)
            {
                foreach (var item in lstDetailCombo)
                {
                    <div class="col-4 mt-3">
                        <div class="card">
                            <div class="card-header"><a href="#"><b>@item.Ten</b></a></div>
                            <div class="d-flex">
                                <img style="height: 120px; width: 200px" src="data:image/jpeg;base64," alt="Alternate Text" />
                                <div class="card-body">
                                    <p>
                                        <b>Loại thức ăn:</b> @item.Loai
                                    </p>
                                    <p>
                                        <b>Giá:</b> @item.GiaBan
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>

    </div>

    <div class="card-footer">
        <button @onclick="Back" class="btn btn-primary">Trở lại</button>
    </div>
</div>




@code {
    [Parameter]
    public string Id { get; set; }
    public List<DetailComboModel> lstDetailCombo { get; set; } = new List<DetailComboModel>();

    protected override async Task OnInitializedAsync()
    {
        try
        {
            if ((await _productDetailService.GetAll()).Any(x => x.Id == Guid.Parse(Id)))
            {
                var obj = await _productDetailService.GetById(Guid.Parse(Id));
                lstDetailCombo = new List<DetailComboModel> {
            new DetailComboModel
            {
                Id = obj.Id,
                Anh = obj.Anh,
                GiaBan = obj.GiaBan,
                Loai = obj.Loai,
                Ten = obj.Ten,
                Ma = obj.Ma
            }
            };
                return;
            }

            var comboDetails = await _combodetailService.GetComboDetail();

            Console.WriteLine("Trước " + comboDetails.Where(item => item.IdCombo == Guid.Parse(Id)).Count());

            var tasks = comboDetails.Where(item => item.IdCombo == Guid.Parse(Id))
    .Select(async item =>
    {
        try
        {
            var obj = await _productDetailService.GetById(item.IdProductDetail);
            var x1 = 10;
            if (obj != null)
            {
                return new DetailComboModel
                {
                    Id = obj.Id,
                    Anh = obj.Anh,
                    GiaBan = obj.GiaBan,
                    Loai = obj.Loai,
                    Ten = obj.Ten,
                    Ma = obj.Ma
                };
            }
        }
        catch (Exception ex)
        {
            Console.WriteLine(ex);
        }
        return null;
    });
            var results = await Task.WhenAll(tasks);
            lstDetailCombo = results.Where(x => x != null).ToList();
        }
        catch (Exception ex)
        {
            Console.WriteLine(ex);
        }
    }

    void Back()
    {
        _navigtion.NavigateTo($"/CartDetails/{Extension.idUser}");
    }
}
