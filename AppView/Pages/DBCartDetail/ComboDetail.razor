@page "/cartdetail/products/{id}"
@using AppData.Models
@using AppData.ViewModes.Combo

@if (lstDetailCombo != null)
{
    foreach (var item in lstDetailCombo)
    {
        <div class="col-4">
            <div class="card">
                <div class="card-header"><a href="#"><b>@item.Ten</b></a></div>
                <div class="d-flex">
                    <img style="height: 120px; width: 200px" src="data:image/jpeg;base64," alt="Alternate Text" />
                    <div class="card-body">
                        <p>
                            <b>Loại thức ăn:</b> @item.Loai
                        </p>
                        <p>
                            <b>Giá:</b> @item.GiaBan
                        </p>
                    </div>
                </div>
            </div>
        </div>
    }
}



